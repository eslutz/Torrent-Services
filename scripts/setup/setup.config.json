                "secrets": [
                    {"field": "cookie", "env": "IPTORRENTS_COOKIE"},
                    {"field": "userAgent", "env": "IPTORRENTS_COOKIE_USER_AGENT"}
                ]
            },
            {
                "name": "1337x (Tor)",
                "priority": 5,
                "tags": ["tor-proxy"],
                "fields": [
